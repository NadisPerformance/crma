generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model User {
  id         Int       @default(autoincrement()) @id
  email      String
  firstname        String?
  lastname     String?
  password   String
  phone  String?
  @@map("users")
}

model Car {
  id         Int       @default(autoincrement()) @id
  immatriculation      String
  @@map("cars")
}

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Car {
  id         Int       @default(autoincrement()) @id
  brandId Int
  model String
  model_date DateTime
  categoryId Int
  price Int
  colorId Int
  plate_number String
  chassis_number String
  availability Boolean
  statusId      Int
  deleted Boolean
  @@map("cars")
}

model User {
  id         Int       @default(autoincrement()) @id
  firstname        String?
  lastname         String?
  gender           String?
  cni              String?
  address          String?
  email            String
  phone            String?
  roleId           Int?
  password         String
  @@map("users")
}

model Brand {
  id         Int       @default(autoincrement()) @id
  name        String
  @@map("brands")
}
